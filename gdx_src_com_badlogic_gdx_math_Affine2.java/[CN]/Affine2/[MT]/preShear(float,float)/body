{
  float tmp00=m00 + shearX * m10;
  float tmp01=m01 + shearX * m11;
  float tmp02=m02 + shearX * m12;
  float tmp10=m10 + shearY * m00;
  float tmp11=m11 + shearY * m01;
  float tmp12=m12 + shearY * m02;
  m00=tmp00;
  m01=tmp01;
  m02=tmp02;
  m10=tmp10;
  m11=tmp11;
  m12=tmp12;
  return this;
}
