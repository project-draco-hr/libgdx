{
  Parallel action=pool.obtain();
  action.actions.clear();
  if (action.finished == null || action.finished.length < actions.size())   action.finished=new boolean[actions.size()];
  int len=actions.size();
  for (int i=0; i < len; i++)   action.finished[i]=false;
  len=actions.size();
  for (int i=0; i < len; i++)   action.actions.add(actions.get(i).copy());
  return action;
}
