{
  int len=actions.size();
  boolean allDone=true;
  for (int i=0; i < len; i++) {
    if (!actions.get(i).isDone()) {
      actions.get(i).act(delta);
    }
 else {
      if (!finished[i]) {
        actions.get(i).finish();
        finished[i]=true;
      }
      allDone&=finished[i];
    }
  }
  if (allDone)   callActionCompletedListener();
}
