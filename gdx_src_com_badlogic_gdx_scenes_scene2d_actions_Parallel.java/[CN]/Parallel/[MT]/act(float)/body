{
  int len=actions.size();
  boolean allDone=true;
  Action action;
  for (int i=0; i < len; i++) {
    action=actions.get(i);
    if (!action.isDone()) {
      action.act(delta);
      allDone=false;
    }
 else {
      if (!finished[i]) {
        finished[i]=true;
        allDone&=finished[i];
      }
    }
  }
  if (allDone)   callActionCompletedListener();
}
