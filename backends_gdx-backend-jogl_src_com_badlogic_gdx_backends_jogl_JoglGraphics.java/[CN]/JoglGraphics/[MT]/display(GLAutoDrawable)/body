{
synchronized (this) {
    if (!paused) {
      updateTimes();
synchronized (((JoglApplication)Gdx.app).runnables) {
        List<Runnable> runnables=((JoglApplication)Gdx.app).runnables;
        for (int i=0; i < runnables.size(); i++) {
          runnables.get(i).run();
        }
        runnables.clear();
      }
      ((JoglInput)Gdx.input).processEvents();
      listener.render();
      ((OpenALAudio)Gdx.audio).update();
    }
  }
}
