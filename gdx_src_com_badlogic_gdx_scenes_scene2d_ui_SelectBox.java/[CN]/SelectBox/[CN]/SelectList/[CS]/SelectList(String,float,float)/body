{
  super(name);
  this.x=x;
  width=SelectBox.this.width;
  height=100;
  this.oldScreenCoords.set(screenCoords);
  layout();
  Stage stage=SelectBox.this.getStage();
  if (y - height < 0 && y + SelectBox.this.height + height < SelectBox.this.getStage().getCamera().viewportHeight)   this.y=y + SelectBox.this.height;
 else   this.y=y - height;
}
