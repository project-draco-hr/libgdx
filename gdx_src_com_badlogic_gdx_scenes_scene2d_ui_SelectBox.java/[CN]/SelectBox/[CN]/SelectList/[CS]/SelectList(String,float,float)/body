{
  super(name);
  setX(x);
  setWidth(SelectBox.this.getWidth());
  setHeight(100);
  this.oldScreenCoords.set(screenCoords);
  layout();
  Stage stage=SelectBox.this.getStage();
  float height=getHeight();
  if (y - height < 0 && y + SelectBox.this.getHeight() + height < SelectBox.this.getStage().getCamera().viewportHeight)   setY(y + SelectBox.this.getHeight());
 else   setY(y - height);
}
