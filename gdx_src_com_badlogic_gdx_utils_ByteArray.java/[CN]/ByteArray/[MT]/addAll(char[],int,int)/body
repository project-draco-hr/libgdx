{
  byte[] items=this.items;
  length*=2;
  int sizeNeeded=size + length;
  if (sizeNeeded > items.length)   items=resize(Math.max(8,(int)(sizeNeeded * 1.75f)));
  for (int i=0, a=0; i < length; i+=2, a++) {
    char value=array[a];
    items[i]=(byte)(value >> 8);
    items[i + 1]=(byte)value;
  }
  size+=length;
}
