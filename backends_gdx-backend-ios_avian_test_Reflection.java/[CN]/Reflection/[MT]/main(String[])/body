{
  Class system=Class.forName("java.lang.System");
  Field out=system.getDeclaredField("out");
  Class output=Class.forName("java.io.PrintStream");
  Method println=output.getDeclaredMethod("println",String.class);
  println.invoke(out.get(null),"Hello, World!");
  expect((Boolean)Reflection.class.getMethod("booleanMethod").invoke(null));
  expect(1 == (Byte)Reflection.class.getMethod("byteMethod").invoke(null));
  expect('2' == (Character)Reflection.class.getMethod("charMethod").invoke(null));
  expect(3 == (Short)Reflection.class.getMethod("shortMethod").invoke(null));
  expect(4 == (Integer)Reflection.class.getMethod("intMethod").invoke(null));
  expect(5.0 == (Float)Reflection.class.getMethod("floatMethod").invoke(null));
  expect(6 == (Long)Reflection.class.getMethod("longMethod").invoke(null));
  expect(7.0 == (Double)Reflection.class.getMethod("doubleMethod").invoke(null));
}
