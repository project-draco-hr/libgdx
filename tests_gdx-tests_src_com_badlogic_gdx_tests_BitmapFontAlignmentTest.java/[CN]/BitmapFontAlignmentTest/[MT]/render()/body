{
  GL10 gl=Gdx.graphics.getGL10();
  gl.glClear(GL10.GL_COLOR_BUFFER_BIT);
  spriteBatch.begin();
  logoSprite.draw(spriteBatch);
switch (renderMode) {
case 0:
    renderSingleLine();
  break;
case 1:
renderSingleLineCached();
break;
case 2:
renderWrapped();
break;
case 3:
renderWrappedCached();
break;
case 4:
renderMultiLine();
break;
case 5:
renderMultiLineCached();
break;
}
spriteBatch.end();
}
