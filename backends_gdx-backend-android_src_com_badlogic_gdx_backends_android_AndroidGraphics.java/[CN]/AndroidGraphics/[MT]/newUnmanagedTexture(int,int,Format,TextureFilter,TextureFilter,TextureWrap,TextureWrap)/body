{
  if (gl != gl20 && (!MathUtils.isPowerOfTwo(width) || !MathUtils.isPowerOfTwo(height)))   throw new GdxRuntimeException("Dimensions have to be a power of two");
  Bitmap.Config config=AndroidPixmap.getInternalFormat(format);
  Bitmap bitmap=Bitmap.createBitmap(width,height,config);
  Texture texture=null;
  texture=new AndroidTexture(this,bitmap,minFilter,magFilter,uWrap,vWrap,false,null);
  bitmap.recycle();
  return texture;
}
