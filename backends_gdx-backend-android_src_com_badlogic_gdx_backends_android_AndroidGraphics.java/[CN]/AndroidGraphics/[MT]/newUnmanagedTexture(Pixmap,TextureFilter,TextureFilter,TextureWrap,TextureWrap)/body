{
  if (gl != gl20 && (!MathUtils.isPowerOfTwo(pixmap.getWidth()) || !MathUtils.isPowerOfTwo(pixmap.getHeight())))   throw new GdxRuntimeException("Dimensions have to be a power of two");
  return new AndroidTexture(this,(Bitmap)pixmap.getNativePixmap(),minFilter,magFilter,uWrap,vWrap,false,null);
}
