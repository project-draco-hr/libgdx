{
  super(null,null,null,name);
  if (stage == null)   throw new IllegalArgumentException("stage cannot be null.");
  if (title == null)   throw new IllegalArgumentException("title cannot be null.");
  this.stage=stage;
  this.title=title;
  this.width=width;
  this.height=height;
  setStyle(style);
  transform=true;
}
