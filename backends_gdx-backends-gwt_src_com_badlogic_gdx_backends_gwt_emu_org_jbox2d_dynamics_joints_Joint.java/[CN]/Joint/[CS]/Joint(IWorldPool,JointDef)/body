{
  assert(def.bodyA != def.bodyB);
  pool=argWorldPool;
  m_type=def.type;
  m_prev=null;
  m_next=null;
  m_bodyA=def.bodyA;
  m_bodyB=def.bodyB;
  m_collideConnected=def.collideConnected;
  m_islandFlag=false;
  m_userData=def.userData;
  m_index=0;
  m_edgeA=new JointEdge();
  m_edgeA.joint=null;
  m_edgeA.other=null;
  m_edgeA.prev=null;
  m_edgeA.next=null;
  m_edgeB=new JointEdge();
  m_edgeB.joint=null;
  m_edgeB.other=null;
  m_edgeB.prev=null;
  m_edgeB.next=null;
}
