{
  BufferedReader in=null;
  try {
    in=new BufferedReader(new InputStreamReader(Gdx.files.external(file).read()));
    soundEnabled=Boolean.parseBoolean(in.readLine());
    for (int i=0; i < 5; i++) {
      highscores[i]=Integer.parseInt(in.readLine());
    }
  }
 catch (  Exception e) {
  }
 finally {
    try {
      if (in != null)       in.close();
    }
 catch (    IOException e) {
    }
  }
}
