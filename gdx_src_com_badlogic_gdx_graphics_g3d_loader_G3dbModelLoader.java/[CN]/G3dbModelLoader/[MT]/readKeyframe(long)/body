{
  long cnt=0;
  final ModelNodeKeyframe keyframe=new ModelNodeKeyframe();
  while ((din.available() > 0) && (cnt < length)) {
    final byte type=din.readByte();
    final long size=readSize(type);
    cnt+=1 + getSize(type) + size;
    if ((type & TYPE_MASK) == G3DB_TAG_TIME)     keyframe.keytime=readFloat(size) / 1000.f;
 else     if ((type & TYPE_MASK) == G3DB_TAG_TRANSLATE)     keyframe.translation=readVector3(size);
 else     if ((type & TYPE_MASK) == G3DB_TAG_ROTATE)     keyframe.rotation=readQuaternion(size);
 else     if ((type & TYPE_MASK) == G3DB_TAG_SCALE)     keyframe.scale=readVector3(size);
 else {
      din.skipBytes((int)size);
      Gdx.app.log("G3dbModelLoader","Unknown keyframe tag: " + (type & TYPE_MASK) + "["+ size+ "], skipping");
    }
  }
  return keyframe;
}
