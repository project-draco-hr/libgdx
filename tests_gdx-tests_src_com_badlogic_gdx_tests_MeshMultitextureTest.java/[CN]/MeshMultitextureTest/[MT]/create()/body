{
  mesh=new Mesh(true,3,0,new VertexAttribute(VertexAttributes.Usage.Color,4,"a_Color"),new VertexAttribute(VertexAttributes.Usage.TextureCoordinates,2,"a_texCoords1"),new VertexAttribute(VertexAttributes.Usage.TextureCoordinates,2,"a_texCoords2"),new VertexAttribute(VertexAttributes.Usage.Position,3,"a_Position"));
