{
  Gdx.gl.glClear(GL10.GL_COLOR_BUFFER_BIT | GL10.GL_DEPTH_BUFFER_BIT);
  Gdx.gl.glEnable(GL10.GL_DEPTH_TEST);
  camera.update();
  for (int i=0; i < decals.size; i++) {
    Decal decal=decals.get(i);
    if (billboard) {
      decal.lookAt(camera.position,camera.up);
    }
    batch.add(decal);
  }
  batch.flush();
  logger.log();
}
