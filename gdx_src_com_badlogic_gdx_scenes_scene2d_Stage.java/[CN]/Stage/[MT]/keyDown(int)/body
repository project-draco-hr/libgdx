{
  if (keyboardFocus == null)   return false;
  InputEvent event=Pools.obtain(InputEvent.class);
  event.setStage(this);
  event.setType(InputEvent.Type.keyDown);
  event.setKeyCode(keyCode);
  keyboardFocus.fire(event);
  boolean handled=event.isHandled();
  Pools.free(event);
  return handled;
}
