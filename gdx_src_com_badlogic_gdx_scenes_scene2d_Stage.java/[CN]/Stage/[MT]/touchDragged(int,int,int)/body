{
  toStageCoordinates(x,y,coords);
  boolean handled=false;
  for (int i=0, n=touchFocus.length; i < n; i++) {
    Actor actor=touchFocus[i];
    if (actor != null) {
      point.set(coords);
      actor.toLocalCoordinates(point);
      actor.touchDragged(point.x,point.y,pointer);
      handled=true;
    }
  }
  return handled;
}
