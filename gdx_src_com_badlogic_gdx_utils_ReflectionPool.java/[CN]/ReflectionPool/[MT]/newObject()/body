{
  if (constructor == null)   throw new RuntimeException("Class cannot be created (missing no-arg constructor): " + constructor.getDeclaringClass().getName());
  try {
    return (T)constructor.newInstance((Object[])null);
  }
 catch (  Exception ex) {
    throw new GdxRuntimeException("Unable to create new instance: " + constructor.getDeclaringClass().getName(),ex);
  }
}
