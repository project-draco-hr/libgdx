{
  AnimatedTiledMapTile.updateAnimationBaseTime();
  beginRender();
  for (  MapLayer layer : map.getLayers()) {
    if (layer.isVisible()) {
      if (layer instanceof TiledMapTileLayer) {
        renderTileLayer((TiledMapTileLayer)layer);
      }
 else {
        for (        MapObject object : layer.getObjects()) {
          renderObject(object);
        }
      }
    }
  }
  endRender();
}
