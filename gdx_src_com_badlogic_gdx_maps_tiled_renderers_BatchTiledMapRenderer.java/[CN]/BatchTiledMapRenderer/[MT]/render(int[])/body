{
  spriteBatch.begin();
  for (  int layerIdx : layers) {
    MapLayer layer=map.getLayers().getLayer(layerIdx);
    if (layer.getVisible()) {
      if (layer instanceof TiledMapTileLayer) {
        renderTileLayer((TiledMapTileLayer)layer);
      }
 else {
        for (        MapObject object : layer.getObjects()) {
          renderObject(object);
        }
      }
    }
  }
  spriteBatch.end();
}
