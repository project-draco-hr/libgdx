{
  NodeList childNodes=parent.getChildNodes();
  for (int i=0; i < childNodes.getLength(); i++) {
    if (childNodes.item(i).getNodeName().equals(child)) {
      return childNodes.item(i);
    }
  }
  if (childNodes.item(0) != null)   return parent.insertBefore(parent.getOwnerDocument().createElement(child),childNodes.item(0));
 else   return parent.appendChild(parent.getOwnerDocument().createElement(child));
}
