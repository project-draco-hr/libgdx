{
  MeshRenderer m=null;
  if (fixed)   m=fpMesh;
 else   m=flMesh;
  for (int i=0; i < 10; i++) {
    m.render(GL10.GL_TRIANGLES);
  }
  if ((System.nanoTime() - startTime) > 1000000000) {
    app.log("Performance",frames + " fps, " + (m.getMesh().getNumVertices() / 3) * frames * 10 + " tris/s");
    frames=0;
    startTime=System.nanoTime();
  }
  frames++;
}
