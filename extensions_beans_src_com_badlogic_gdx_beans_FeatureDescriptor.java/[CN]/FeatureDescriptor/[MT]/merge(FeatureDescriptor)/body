{
  assert(name.equals(feature.name));
  expert|=feature.expert;
  hidden|=feature.hidden;
  preferred|=feature.preferred;
  if (shortDescription == null) {
    shortDescription=feature.shortDescription;
  }
  if (name == null) {
    name=feature.name;
  }
  if (displayName == null) {
    displayName=feature.displayName;
  }
}
