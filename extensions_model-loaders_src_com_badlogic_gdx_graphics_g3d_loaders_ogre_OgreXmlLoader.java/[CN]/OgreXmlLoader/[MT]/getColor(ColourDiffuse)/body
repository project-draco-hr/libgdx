{
  String[] tokens=colourDiffuse.getValue().split(" ");
  if (tokens.length == 3)   color.set(Float.valueOf(tokens[0]),Float.valueOf(tokens[1]),Float.valueOf(tokens[2]),1);
 else   color.set(Float.valueOf(tokens[0]),Float.valueOf(tokens[1]),Float.valueOf(tokens[2]),Float.valueOf(tokens[3]));
  return color.toFloatBits();
}
