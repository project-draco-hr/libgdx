{
  if (!MathUtils.isPowerOfTwo(pixmap.getHeight()) || !MathUtils.isPowerOfTwo(pixmap.getWidth()))   throw new GdxRuntimeException("Texture dimensions must be a power of two: " + width + "x"+ height);
  return new AngleTexture((BufferedImage)pixmap.getNativePixmap(),minFilter,magFilter,uWrap,vWrap,false);
}
