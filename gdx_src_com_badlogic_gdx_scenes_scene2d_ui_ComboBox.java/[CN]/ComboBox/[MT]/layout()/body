{
  final NinePatch background=style.background;
  final BitmapFont font=style.font;
  prefHeight=Math.max(background.getTopHeight() + background.getBottomHeight() + font.getCapHeight() - font.getDescent() * 2,background.getTotalHeight());
  float max=0;
  for (int i=0; i < entries.length; i++) {
    max=Math.max(font.getBounds(entries[i]).width,max);
  }
  prefWidth=background.getLeftWidth() + background.getRightWidth() + max;
  invalidated=false;
}
