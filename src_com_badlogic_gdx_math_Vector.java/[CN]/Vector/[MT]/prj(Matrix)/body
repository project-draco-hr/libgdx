{
  float l_mat[]=matrix.val;
  float l_w=val[0] * l_mat[Matrix.M30] + val[1] * l_mat[Matrix.M31] + val[2] * l_mat[Matrix.M32] + l_mat[Matrix.M33];
  return this.set((val[0] * l_mat[Matrix.M00] + val[1] * l_mat[Matrix.M01] + val[2] * l_mat[Matrix.M02] + l_mat[Matrix.M03]) / l_w,(val[0] * l_mat[Matrix.M10] + val[1] * l_mat[Matrix.M11] + val[2] * l_mat[Matrix.M12] + l_mat[Matrix.M13]) / l_w,(val[0] * l_mat[Matrix.M20] + val[1] * l_mat[Matrix.M21] + val[2] * l_mat[Matrix.M22] + l_mat[Matrix.M23]) / l_w);
}
