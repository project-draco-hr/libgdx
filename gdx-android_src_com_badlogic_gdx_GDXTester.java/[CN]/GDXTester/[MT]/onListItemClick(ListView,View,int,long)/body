{
  super.onListItemClick(l,v,position,id);
  Object o=this.getListAdapter().getItem(position);
  String keyword=o.toString();
  Intent intent=null;
  if (keyword.equals(items[0]))   intent=new Intent(this,FixedPointTest.class);
  if (keyword.equals(items[1]))   intent=new Intent(this,FloatTest.class);
  startActivity(intent);
}
