{
  if (complete)   return true;
  complete=true;
  Pool pool=getPool();
  setPool(null);
  try {
    Array<Action> actions=this.actions;
    for (int i=0, n=actions.size; i < n && actor != null; i++) {
      if (currentAction.getActor() != null && !currentAction.act(delta))       complete=false;
      if (actor == null)       return true;
      if (actor == null)       return true;
    }
    return complete;
  }
  finally {
    setPool(pool);
  }
}
