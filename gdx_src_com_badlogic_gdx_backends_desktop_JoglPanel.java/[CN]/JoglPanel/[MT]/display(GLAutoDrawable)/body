{
  if (multiplexer != null)   multiplexer.processEvents();
  deltaTime=(System.nanoTime() - frameStart) / 1000000000.0f;
  frameStart=System.nanoTime();
  mean.addValue(deltaTime);
synchronized (this) {
    for (    RenderListener listener : setupListeners)     listener.surfaceCreated(application);
    listeners.addAll(setupListeners);
    setupListeners.clear();
    for (    RenderListener listener : listeners)     listener.render(application);
  }
}
