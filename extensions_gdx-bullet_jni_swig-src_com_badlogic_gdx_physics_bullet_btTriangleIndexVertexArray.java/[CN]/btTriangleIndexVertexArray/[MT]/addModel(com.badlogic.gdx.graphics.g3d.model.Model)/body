{
  for (int i=0; i < models.length; i++) {
    for (int j=0; j < models[i].meshParts.size; j++) {
      com.badlogic.gdx.graphics.g3d.model.MeshPart mp=models[i].meshParts.get(j);
      if (mp.primitiveType == com.badlogic.gdx.graphics.GL10.GL_TRIANGLES)       addIndexedMesh(new btIndexedMesh(mp.mesh,mp.indexOffset,mp.numVertices),PHY_ScalarType.PHY_SHORT,true);
    }
  }
}
