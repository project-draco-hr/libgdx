{
  if (gl != gl20 && (!isPowerOfTwo(width) || !isPowerOfTwo(height)))   throw new GdxRuntimeException("Texture dimensions must be a power of two");
  if (format == Format.Alpha)   return new JoglTexture(width,height,BufferedImage.TYPE_BYTE_GRAY,minFilter,magFilter,uWrap,vWrap,false);
 else   return new JoglTexture(width,height,BufferedImage.TYPE_4BYTE_ABGR,minFilter,magFilter,uWrap,vWrap,false);
}
