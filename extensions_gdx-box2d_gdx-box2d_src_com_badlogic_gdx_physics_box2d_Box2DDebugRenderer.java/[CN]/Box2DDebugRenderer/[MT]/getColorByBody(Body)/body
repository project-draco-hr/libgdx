{
  if (body.isActive() == false)   return SHAPE_NOT_ACTIVE;
 else   if (body.getType() == BodyType.StaticBody)   return SHAPE_STATIC;
 else   if (body.getType() == BodyType.KinematicBody)   return SHAPE_KINEMATIC;
 else   if (body.isAwake() == false)   return SHAPE_NOT_AWAKE;
 else   return SHAPE_AWAKE;
}
