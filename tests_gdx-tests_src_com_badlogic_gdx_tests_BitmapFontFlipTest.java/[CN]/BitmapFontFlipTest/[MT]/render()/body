{
  alpha=(alpha + Gdx.graphics.getDeltaTime() * 0.1f) % 1;
  GL10 gl=Gdx.graphics.getGL10();
  gl.glClear(GL10.GL_COLOR_BUFFER_BIT);
  spriteBatch.begin();
  logoSprite.draw(spriteBatch);
switch (renderMode) {
case 0:
    renderNormal();
  break;
case 1:
renderCached();
break;
}
spriteBatch.end();
Gdx.input.processEvents(inputProcessor);
}
