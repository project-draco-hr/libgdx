{
  if (!touchable)   return;
  point.x=x;
  point.y=y;
  Actor actor=focusedActor[pointer];
  if (actor == null) {
    parent.focus(null,pointer);
    return;
  }
  if (actor != this) {
    actor.toLocalCoordinates(point);
    actor.touchUp(point.x,point.y,pointer);
  }
  if (focusedActor[pointer] == actor)   actor.parent.focus(null,pointer);
}
