{
  int len=children.size() - 1;
  for (int i=len; i >= 0; i--) {
    Actor child=children.get(i);
    toChildCoordinates(child,x,y,point);
    Actor hit=child.hit(point.x,point.y);
    if (hit != null) {
      return hit;
    }
  }
  return x > 0 && x < width && y > 0 && y < height ? this : null;
}
