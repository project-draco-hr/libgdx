{
  float dx1=cx1 - x1;
  float dy1=cy1 - y1;
  float dx2=cx2 - cx1;
  float dy2=cy2 - cy1;
  float dx3=x2 - cx2;
  float dy3=y2 - cy2;
  float length=(float)Math.sqrt(dx1 * dx1 + dy1 * dy1) + (float)Math.sqrt(dx2 * dx2 + dy2 * dy2) + (float)Math.sqrt(dx3 * dx3 + dy3 * dy3);
  curve(x1,y1,cx1,cy1,cx2,cy2,x2,y2,4 * (int)Math.cbrt(length));
}
