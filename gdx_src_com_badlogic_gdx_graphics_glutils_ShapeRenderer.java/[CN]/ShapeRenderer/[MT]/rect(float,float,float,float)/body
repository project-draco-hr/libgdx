{
  check(ShapeType.Line,ShapeType.Filled,8);
  if (shapeType == ShapeType.Line) {
    renderer.color(color);
    renderer.vertex(x,y,0);
    renderer.color(color);
    renderer.vertex(x + width,y,0);
    renderer.color(color);
    renderer.vertex(x + width,y,0);
    renderer.color(color);
    renderer.vertex(x + width,y + height,0);
    renderer.color(color);
    renderer.vertex(x + width,y + height,0);
    renderer.color(color);
    renderer.vertex(x,y + height,0);
    renderer.color(color);
    renderer.vertex(x,y + height,0);
    renderer.color(color);
    renderer.vertex(x,y,0);
  }
 else {
    renderer.color(color);
    renderer.vertex(x,y,0);
    renderer.color(color);
    renderer.vertex(x + width,y,0);
    renderer.color(color);
    renderer.vertex(x + width,y + height,0);
    renderer.color(color);
    renderer.vertex(x + width,y + height,0);
    renderer.color(color);
    renderer.vertex(x,y + height,0);
    renderer.color(color);
    renderer.vertex(x,y,0);
  }
}
