{
  originX+=xAmount;
  originY+=yAmount;
  float x=vertices[X1] + xAmount;
  float y=vertices[Y1] + yAmount;
  float x2=x + width;
  float y2=y + height;
  vertices[X1]=x;
  vertices[Y1]=y;
  vertices[X2]=x;
  vertices[Y2]=y2;
  vertices[X3]=x2;
  vertices[Y3]=y2;
  vertices[X4]=x2;
  vertices[Y4]=y2;
  vertices[X5]=x2;
  vertices[Y5]=y;
  vertices[X6]=x;
  vertices[Y6]=y;
}
