{
  Glyph lastGlyph=null;
  while (start < end) {
    lastGlyph=getGlyph(str.charAt(start++));
    if (lastGlyph != null) {
      cache.addGlyph(lastGlyph,x,y,color);
      x+=lastGlyph.xadvance;
      break;
    }
  }
  while (start < end) {
    char ch=str.charAt(start++);
    Glyph g=getGlyph(ch);
    if (g != null) {
      x+=lastGlyph.getKerning(ch);
      lastGlyph=g;
      cache.addGlyph(lastGlyph,x,y,color);
      x+=g.xadvance;
    }
  }
}
