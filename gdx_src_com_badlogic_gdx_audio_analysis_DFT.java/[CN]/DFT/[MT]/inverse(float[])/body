{
  int N=buffer.length;
  real[0]/=N;
  imag[0]=-imag[0] / (N / 2);
  real[N / 2]/=N;
  imag[N / 2]=-imag[0] / (N / 2);
  for (int i=0; i < N / 2; i++) {
    real[i]/=(N / 2);
    imag[i]=-imag[i] / (N / 2);
  }
  for (int t=0; t < N; t++) {
    buffer[t]=0.0f;
    for (int f=0; f < N / 2; f++) {
      buffer[t]+=real[f] * cos(t * f) + imag[f] * sin(t * f);
    }
  }
}
