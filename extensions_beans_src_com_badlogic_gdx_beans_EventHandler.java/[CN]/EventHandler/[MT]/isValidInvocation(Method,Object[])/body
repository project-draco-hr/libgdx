{
  if (listenerMethodName == null) {
    return true;
  }
  if (listenerMethodName.equals(method.getName())) {
    if (eventPropertyName == null && (arguments == null || arguments.length == 0)) {
      return true;
    }
    if (arguments != null && arguments.length == 1) {
      return true;
    }
  }
  return false;
}
