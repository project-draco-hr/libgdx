{
  assert(childIndex < m_count);
  int i1=childIndex;
  int i2=childIndex + 1;
  if (i2 == m_count) {
    i2=0;
  }
  final Vec2 v1=pool1;
  final Vec2 v2=pool2;
  Transform.mulToOutUnsafe(xf,m_vertices[i1],v1);
  Transform.mulToOutUnsafe(xf,m_vertices[i2],v2);
  Vec2.minToOut(v1,v2,aabb.lowerBound);
  Vec2.maxToOut(v1,v2,aabb.upperBound);
}
