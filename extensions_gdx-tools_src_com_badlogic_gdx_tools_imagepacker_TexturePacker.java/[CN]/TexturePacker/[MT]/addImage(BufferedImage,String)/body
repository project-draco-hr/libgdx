{
  Image squeezed=squeeze(image,name,false);
  if (squeezed != null) {
    if (settings.alias) {
      String crc=hash(squeezed);
      Image existing=imageCrcs.get(crc);
      if (existing != null) {
        existing.aliases.add(squeezed);
        return;
      }
      imageCrcs.put(crc,squeezed);
    }
    images.add(squeezed);
  }
}
