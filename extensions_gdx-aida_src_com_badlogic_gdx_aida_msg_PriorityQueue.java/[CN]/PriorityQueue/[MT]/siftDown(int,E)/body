{
  Comparable<? super E> key=(Comparable<? super E>)x;
  int half=size >>> 1;
  while (k < half) {
    int child=(k << 1) + 1;
    Object c=queue[child];
    int right=child + 1;
    if (right < size && ((Comparable<? super E>)c).compareTo((E)queue[right]) > 0)     c=queue[child=right];
    if (key.compareTo((E)c) <= 0)     break;
    queue[k]=c;
    k=child;
  }
  queue[k]=key;
}
