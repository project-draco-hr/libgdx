{
  if (!material.has(TextureAttribute.Diffuse))   material.add(textureAttribute);
 else   if (!material.has(ColorAttribute.Diffuse))   material.add(colorAttribute);
 else   if (!material.has(BlendingAttribute.Type))   material.add(blendingAttribute);
 else   material.clear();
  return super.touchUp(screenX,screenY,pointer,button);
}
