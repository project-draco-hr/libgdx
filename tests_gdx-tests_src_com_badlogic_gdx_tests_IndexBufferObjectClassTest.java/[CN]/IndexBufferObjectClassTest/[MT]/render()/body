{
  GL11 gl=Gdx.gl11;
  gl.glViewport(0,0,Gdx.graphics.getWidth(),Gdx.graphics.getHeight());
  gl.glClear(GL10.GL_COLOR_BUFFER_BIT);
  gl.glEnable(GL10.GL_TEXTURE_2D);
  texture.bind();
  vbo.bind();
  ibo.bind();
  gl.glDrawElements(GL11.GL_TRIANGLES,3,GL11.GL_UNSIGNED_SHORT,0);
  ibo.unbind();
  vbo.unbind();
}
