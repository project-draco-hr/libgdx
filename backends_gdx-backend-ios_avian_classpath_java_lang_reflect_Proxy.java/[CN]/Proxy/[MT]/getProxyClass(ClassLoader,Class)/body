{
  for (  Class c : interfaces) {
    if (!c.isInterface()) {
      throw new IllegalArgumentException();
    }
  }
  int number;
synchronized (Proxy.class) {
    number=nextNumber++;
  }
  try {
    return makeClass(loader,interfaces,"Proxy-" + number);
  }
 catch (  IOException e) {
    AssertionError error=new AssertionError();
    error.initCause(e);
    throw error;
  }
}
