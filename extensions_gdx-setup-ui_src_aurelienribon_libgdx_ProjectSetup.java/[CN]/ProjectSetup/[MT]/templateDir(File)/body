{
  String[] filteredExts=new String[]{".jar",".zip",".png"};
  for (  File file : dir.listFiles()) {
    if (file.isDirectory()) {
      templateDir(file);
    }
 else {
      boolean isValid=true;
      for (      String filter : filteredExts) {
        if (file.getName().endsWith(filter)) {
          isValid=false;
          break;
        }
      }
      if (isValid)       templateManager.processOver(file);
    }
  }
}
