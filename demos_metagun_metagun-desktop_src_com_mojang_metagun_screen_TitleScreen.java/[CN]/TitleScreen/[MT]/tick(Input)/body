{
  time++;
  if (time > 240) {
    if (input.buttons[Input.SHOOT] && !input.oldButtons[Input.SHOOT] || Gdx.input.isTouched()) {
      Sound.startgame.play();
      setScreen(new GameScreen());
      input.releaseAllKeys();
    }
  }
  if (time > 60 * 10) {
    setScreen(new ExpositionScreen());
  }
}
