{
  T[] values=this.values;
  if (size == values.length) {
    if (!resizable) {
      throw new IllegalStateException("Queue is full");
    }
 else {
      resize(values.length << 1);
      values=this.values;
    }
  }
  values[tail++]=object;
  if (tail == values.length) {
    tail=0;
  }
  size++;
}
