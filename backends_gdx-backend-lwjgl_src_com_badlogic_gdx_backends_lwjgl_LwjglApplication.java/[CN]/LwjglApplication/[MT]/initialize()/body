{
  mainLoopThread=new Thread("LWJGL Application"){
    public void run(){
      graphics.setVSync(graphics.config.vSyncEnabled);
      try {
        LwjglApplication.this.mainLoop();
      }
 catch (      Throwable t) {
        if (audio != null)         audio.dispose();
        throw new GdxRuntimeException(t);
      }
    }
  }
;
  mainLoopThread.start();
}
