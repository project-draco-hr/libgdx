{
  min.set(minimum.getX() < maximum.getX() ? minimum.getX() : maximum.getX(),minimum.getY() < maximum.getY() ? minimum.getY() : maximum.getY(),minimum.getZ() < maximum.getZ() ? minimum.getZ() : maximum.getZ());
  max.set(minimum.getX() > maximum.getX() ? minimum.getX() : maximum.getX(),minimum.getY() > maximum.getY() ? minimum.getY() : maximum.getY(),minimum.getZ() > maximum.getZ() ? minimum.getZ() : maximum.getZ());
  cnt.set(min).add(max).mul(0.5f);
  dim.set(max).sub(min);
  crn_dirty=true;
  return this;
}
