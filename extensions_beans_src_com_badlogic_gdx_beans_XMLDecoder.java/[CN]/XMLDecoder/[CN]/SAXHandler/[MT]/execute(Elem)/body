{
  if (elem.isExecuted) {
    return elem.result;
  }
  try {
    if (elem.idref != null) {
      elem.result=idObjMap.get(elem.idref);
    }
 else     if (elem.isBasicType) {
      elem.result=executeBasic(elem);
    }
 else {
      elem.result=executeCommon(elem);
    }
  }
 catch (  Exception e) {
    listener.exceptionThrown(e);
  }
  if (elem.id != null) {
    idObjMap.put(elem.id,elem.result);
  }
  elem.isExecuted=true;
  return elem.result;
}
