{
  if (listenerInitialized == false) {
    initializeListener();
  }
synchronized (runnables) {
    executedRunnables.addAll(runnables);
    runnables.clear();
  }
  for (  Runnable runnable : executedRunnables) {
    runnable.run();
  }
  executedRunnables.clear();
  graphics.update();
  listener.render();
  glfwSwapBuffers(windowHandle);
  input.update();
  glfwPollEvents();
}
