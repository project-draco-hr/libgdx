{
  if (value == null || value instanceof Number || value instanceof Boolean)   return String.valueOf(value);
  String string=String.valueOf(value).replace("\\","\\\\").replace("\r","\\r").replace("\n","\\n").replace("\t","\\t");
  if (this == OutputType.minimal && !string.equals("true") && !string.equals("false") && !string.equals("null") && !string.contains("//") && !string.contains("/*") && minimalValuePattern.matcher(string).matches())   return string;
  return '"' + string.replace("\"","\\\"") + '"';
}
