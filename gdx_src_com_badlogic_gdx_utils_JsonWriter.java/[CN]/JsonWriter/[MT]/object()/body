{
  if (current != null) {
    if (current.array) {
      if (!current.needsComma)       current.needsComma=true;
 else       writer.write(',');
    }
 else {
      if (!named && !current.array)       throw new IllegalStateException("Name must be set.");
      named=false;
    }
  }
  stack.push(current=new JsonObject(false));
  return this;
}
