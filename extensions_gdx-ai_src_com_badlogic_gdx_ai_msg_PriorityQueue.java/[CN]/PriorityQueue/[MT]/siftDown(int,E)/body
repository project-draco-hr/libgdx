{
  int half=size >>> 1;
  while (k < half) {
    int child=(k << 1) + 1;
    E c=(E)queue[child];
    int right=child + 1;
    if (right < size && c.compareTo((E)queue[right]) > 0)     c=(E)queue[child=right];
    if (x.compareTo(c) <= 0)     break;
    queue[k]=c;
    k=child;
  }
  queue[k]=x;
}
