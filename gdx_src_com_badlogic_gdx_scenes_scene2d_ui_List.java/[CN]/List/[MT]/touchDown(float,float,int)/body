{
  if (pointer != 0)   return false;
  if (hit(x,y) != null) {
    selected=(int)((height - y) / entryHeight);
    selected=Math.max(0,selected);
    selected=Math.min(entries.length - 1,selected);
    if (listener != null)     listener.selected(this,selected,entries[selected]);
    return true;
  }
  return false;
}
