{
  Chip8 chip=new Chip8();
  Chip8Assembler asm=new Chip8Assembler(chip);
  asm.ldRegVal(0,1);
  asm.ldRegVal(1,2);
  asm.addRegReg(0,1);
  chip.tick(3);
  chip.debugPrint();
  asm.ldRegVal(0,13);
  asm.shlReg(0);
  chip.tick(2);
  chip.debugPrint();
  asm.shrReg(0);
  chip.tick(1);
  chip.debugPrint();
}
