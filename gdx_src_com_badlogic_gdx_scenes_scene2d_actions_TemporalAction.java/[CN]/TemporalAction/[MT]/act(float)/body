{
  if (complete)   return true;
  Pool pool=getPool();
  setPool(null);
  try {
    if (!begun) {
      begin();
      begun=true;
    }
    time+=delta;
    complete=time >= duration;
    float percent;
    if (complete)     percent=1;
 else {
      percent=time / duration;
      if (interpolation != null)       percent=interpolation.apply(percent);
    }
    update(reverse ? 1 - percent : percent);
    if (complete)     end();
    return complete;
  }
  finally {
    setPool(pool);
  }
}
