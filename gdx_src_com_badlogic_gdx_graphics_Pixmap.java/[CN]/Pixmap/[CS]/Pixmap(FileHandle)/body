{
  InputStream in=null;
  try {
    in=file.read();
    pixmap=new Gdx2DPixmap(in,0);
  }
 catch (  Exception e) {
    throw new GdxRuntimeException("Couldn't load file: " + file,e);
  }
 finally {
    if (in != null)     ;
    try {
      in.close();
    }
 catch (    Exception e) {
    }
    ;
  }
}
