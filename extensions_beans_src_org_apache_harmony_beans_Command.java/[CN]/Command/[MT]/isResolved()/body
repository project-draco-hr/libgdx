{
  if (getStatus() < Command.CHILDREN_PROCESSED) {
    return false;
  }
  for (int i=0; i < arguments.size(); ++i) {
    Command arg=arguments.elementAt(i);
    if (!arg.isResolved()) {
      return false;
    }
  }
  for (int j=0; j < operations.size(); ++j) {
    Command opr=operations.elementAt(j);
    if (!opr.isResolved()) {
      return false;
    }
  }
  return true;
}
