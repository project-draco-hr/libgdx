{
  sprite.setColor(tintColor.getRedFloat(),tintColor.getGreenFloat(),tintColor.getBlueFloat(),tintColor.getAlphaFloat());
  if (repeatCountX * this.width != width || repeatCountY * this.height != height)   drawRepeatSlow(x,y,width,height,repeatCountX,repeatCountY);
 else   drawRepeat(x,y,repeatCountX,repeatCountY);
}
