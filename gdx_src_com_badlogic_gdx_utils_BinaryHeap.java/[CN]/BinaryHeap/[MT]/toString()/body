{
  if (size == 0)   return "[]";
  Object[] nodes=this.nodes;
  StringBuilder buffer=new StringBuilder(32);
  buffer.append('[');
  buffer.append(nodes[0]);
  for (int i=1; i < size; i++) {
    buffer.append(", ");
    buffer.append(nodes[i]);
  }
  buffer.append(']');
  return buffer.toString();
}
