{
  Writer writer=null;
  try {
    writer=file.writer(false);
    toJson(object,knownType,elementType,writer);
  }
 catch (  Exception ex) {
    throw new SerializationException("Error writing file: " + file,ex);
  }
 finally {
    try {
      if (writer != null)       writer.close();
    }
 catch (    IOException ignored) {
    }
  }
}
