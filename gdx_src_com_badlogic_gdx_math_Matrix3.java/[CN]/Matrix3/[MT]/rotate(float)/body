{
  if (angle == 0)   return this;
  angle=DEGREE_TO_RAD * angle;
  float cos=(float)Math.cos(angle);
  float sin=(float)Math.sin(angle);
  tmp[M00]=cos;
  tmp[M10]=sin;
  tmp[M20]=0;
  tmp[M01]=-sin;
  tmp[M11]=cos;
  tmp[M21]=0;
  tmp[M02]=0;
  tmp[M12]=0;
  tmp[M22]=1;
  mul(val,tmp);
  return this;
}
