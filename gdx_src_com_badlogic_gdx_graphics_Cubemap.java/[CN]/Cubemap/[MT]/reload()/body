{
  if (!canReload())   throw new GdxRuntimeException("Tried to reload an unmanaged Cubemap");
  glHandle=createGLHandle();
  load(data[CubemapSide.PositiveX.index],data[CubemapSide.NegativeX.index],data[CubemapSide.PositiveY.index],data[CubemapSide.NegativeY.index],data[CubemapSide.PositiveZ.index],data[CubemapSide.NegativeZ.index]);
}
