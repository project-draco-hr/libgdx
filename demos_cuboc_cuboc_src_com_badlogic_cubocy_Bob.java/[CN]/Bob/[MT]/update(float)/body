{
  processKeys();
  accel.y=-GRAVITY;
  accel.mul(deltaTime);
  vel.add(accel.x,accel.y);
  if (accel.x == 0)   vel.x*=DAMP;
  if (vel.x > MAX_VEL)   vel.x=MAX_VEL;
  if (vel.x < -MAX_VEL)   vel.x=-MAX_VEL;
  vel.mul(deltaTime);
  tryMove();
  vel.mul(1.0f / deltaTime);
  if (state == SPAWN) {
    if (stateTime > 0.4f) {
      state=IDLE;
    }
  }
  if (state == DYING) {
    if (stateTime > 0.4f) {
      state=DEAD;
    }
  }
  stateTime+=deltaTime;
}
