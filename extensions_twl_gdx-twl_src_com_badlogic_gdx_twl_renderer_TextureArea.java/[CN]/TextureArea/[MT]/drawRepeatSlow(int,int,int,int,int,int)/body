{
  while (repeatCountY > 0) {
    int rowHeight=height / repeatCountY;
    int cx=0;
    for (int xi=0; xi < repeatCountX; ) {
      int nx=++xi * width / repeatCountX;
      drawQuad(x + cx,y,nx - cx,rowHeight);
      cx=nx;
    }
    y+=rowHeight;
    height-=rowHeight;
    repeatCountY--;
  }
}
