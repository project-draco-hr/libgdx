{
  super.OnRenderFrame(arg0);
  if (paused)   return;
  long time=System.nanoTime();
  deltaTime=(time - lastFrameTime) / 1000000000.0f;
  lastFrameTime=time;
  frames++;
  if (time - framesStart >= 1000000000l) {
    framesStart=time;
    fps=frames;
    frames=0;
  }
  MakeCurrent();
  ((IOSInput)Gdx.input).processEvents();
  app.listener.render();
  SwapBuffers();
}
