{
  Entry[] table=this.table;
  if (identity || value == null) {
    for (int i=table.length; i-- > 0; )     for (Entry e=table[i]; e != null; e=e.next)     if (e.value == value)     return true;
  }
 else {
    for (int i=table.length; i-- > 0; )     for (Entry e=table[i]; e != null; e=e.next)     if (value.equals(e.value))     return true;
  }
  return false;
}
