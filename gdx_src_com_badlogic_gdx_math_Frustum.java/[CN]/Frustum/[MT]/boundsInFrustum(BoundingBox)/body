{
  Vector3[] corners=bounds.getCorners();
  int len=corners.length;
  for (int i=0, len2=planes.size(); i < len2; i++) {
    int out=0;
    for (int j=0; j < len; j++)     if (planes.get(i).testPoint(corners[j]) == PlaneSide.Back)     out++;
    if (out == 8)     return false;
  }
  return true;
}
