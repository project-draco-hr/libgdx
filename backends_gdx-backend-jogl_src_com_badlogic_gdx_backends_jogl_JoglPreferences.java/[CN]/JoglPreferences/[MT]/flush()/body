{
  OutputStream out=null;
  try {
    out=new BufferedOutputStream(Gdx.files.external(JoglPreferences.this.name).write(false));
    properties.storeToXML(out,null);
  }
 catch (  Throwable t) {
  }
 finally {
    if (out != null)     try {
      out.close();
    }
 catch (    Exception e) {
    }
  }
}
