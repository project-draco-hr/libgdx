def index_sort(s1, s2):
    if (not s1):
        return (-1)
    if (not s2):
        return 1
    l1 = len(s1)
    l2 = len(s2)
    m1 = string.lower(s1)
    m2 = string.lower(s2)
    for i in range(l1):
        if ((i >= l2) or (m1[i] > m2[i])):
            return 1
        if (m1[i] < m2[i]):
            return (-1)
        if (s1[i] < s2[i]):
            return (-1)
        if (s1[i] > s2[i]):
            return 1
    if (l2 > l1):
        return (-1)
    return 0
