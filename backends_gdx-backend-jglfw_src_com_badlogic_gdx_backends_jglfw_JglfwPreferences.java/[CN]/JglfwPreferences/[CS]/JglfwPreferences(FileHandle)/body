{
  this.name=file.name();
  this.file=file;
  if (!file.exists())   return;
  InputStream in=null;
  try {
    in=new BufferedInputStream(file.read());
    properties.load(in);
  }
 catch (  Throwable ex) {
    ex.printStackTrace();
  }
 finally {
    if (in != null) {
      try {
        in.close();
      }
 catch (      Exception ignored) {
      }
    }
  }
}
