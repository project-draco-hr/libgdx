{
  this.model=model;
  this.shape=shape;
  if (shape != null) {
    Vector3 localInertia;
    if (mass == 0)     localInertia=Vector3.Zero;
 else {
      shape.calculateLocalInertia(mass,Vector3.tmp);
      localInertia=Vector3.tmp;
    }
    bodyInfo=new btRigidBodyConstructionInfo(mass,null,shape,localInertia);
  }
}
