{
  int newGlyphCount=end - start;
  if (vertexData.length == 1) {
    require(0,newGlyphCount);
  }
 else {
    for (int i=0; i < tmpGlyphCount.length; i++)     tmpGlyphCount[i]=0;
    while (start < end) {
      Glyph g=font.data.getGlyph(seq.charAt(start++));
      if (g == null)       continue;
      tmpGlyphCount[g.page]++;
    }
    for (int i=0; i < tmpGlyphCount.length; i++)     require(i,tmpGlyphCount[i]);
  }
}
