{
  int[] rgbArray=new int[tile.getWidth() * tile.getHeight()];
  tile.getRGB(0,0,tile.getWidth(),tile.getHeight(),rgbArray,0,tile.getWidth());
  for (int i=0; i < tile.getWidth() * tile.getHeight(); i++) {
    if (((rgbArray[i] >> 24) & 0xff) != 255) {
      return true;
    }
  }
  return false;
}
