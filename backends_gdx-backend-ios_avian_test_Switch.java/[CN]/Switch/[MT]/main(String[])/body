{
  expect(table(0) == 0);
  expect(table(9) == 9);
  expect(table(10) == 10);
  expect(table(11) == 11);
  expect(table(12) == 8);
  expect(table(-5) == 5);
  expect(table(-13) == 7);
  expect(lookup(0) == 0);
  expect(lookup(45) == 45);
  expect(lookup(46) == 46);
  expect(lookup(47) == -47);
  expect(lookup(245) == 245);
  expect(lookup(246) == 91);
}
