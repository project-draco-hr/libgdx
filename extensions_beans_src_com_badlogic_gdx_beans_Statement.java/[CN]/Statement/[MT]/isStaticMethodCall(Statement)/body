{
  Object target=stmt.getTarget();
  String methodName=stmt.getMethodName();
  if (!(target instanceof Class<?>)) {
    return false;
  }
  try {
    Statement.findMethod((Class<?>)target,methodName,stmt.getArguments(),true);
    return true;
  }
 catch (  NoSuchMethodException e) {
    return false;
  }
}
