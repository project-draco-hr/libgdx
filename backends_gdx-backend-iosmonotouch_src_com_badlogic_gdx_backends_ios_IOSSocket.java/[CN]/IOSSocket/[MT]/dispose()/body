{
  if (stream != null) {
    try {
      stream.Close();
    }
 catch (    Exception e) {
      throw new GdxRuntimeException("Error closing stream.",e);
    }
    stream=null;
  }
  if (server != null) {
    try {
      server.dispose(this);
    }
 catch (    Exception e) {
      throw new GdxRuntimeException("Error closing socket on server.",e);
    }
    server=null;
  }
  if (client != null) {
    try {
      client.Close();
      client.Dispose();
    }
 catch (    Exception e) {
      throw new GdxRuntimeException("Error closing socket.",e);
    }
    client=null;
  }
}
