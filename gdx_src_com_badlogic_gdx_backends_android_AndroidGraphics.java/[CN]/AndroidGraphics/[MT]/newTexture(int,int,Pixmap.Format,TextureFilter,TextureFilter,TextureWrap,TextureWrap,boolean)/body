{
  Bitmap.Config config=AndroidPixmap.getInternalFormat(format);
  Bitmap bitmap=Bitmap.createBitmap(width,height,config);
  Texture texture=null;
  if (gl10 != null)   texture=new AndroidTexture(this,gl10,bitmap,minFilter,magFilter,uWrap,vWrap,managed);
 else   texture=new AndroidTexture(this,gl20,bitmap,minFilter,magFilter,uWrap,vWrap,managed);
  if (!managed)   bitmap.recycle();
  return texture;
}
