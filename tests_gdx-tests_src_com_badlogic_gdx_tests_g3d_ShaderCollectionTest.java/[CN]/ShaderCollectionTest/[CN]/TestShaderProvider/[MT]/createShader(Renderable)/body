{
  return createShader(vertexShader,fragmentShader,renderable,renderable.lights != null,renderable.lights != null && renderable.lights.environmentCubemap != null,renderable.lights != null && renderable.lights.shadowMap != null,renderable.lights != null && renderable.lights.fog != null,2,5,3,renderable.bones == null ? 0 : 12);
