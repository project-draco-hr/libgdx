{
  StringBuilder sb=new StringBuilder();
  if (!valueIsDefined) {
    sb.append("<unbound>");
  }
 else {
    if (value == null) {
      sb.append(BeansUtils.NULL);
    }
 else {
      Class<?> clazz=value.getClass();
      sb.append(clazz == String.class ? BeansUtils.QUOTE : BeansUtils.idOfClass(clazz));
    }
  }
  sb.append('=');
  sb.append(super.toString());
  return sb.toString();
}
