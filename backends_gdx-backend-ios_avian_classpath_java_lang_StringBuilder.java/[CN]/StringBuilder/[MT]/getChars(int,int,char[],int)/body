{
  if (srcStart < 0 || srcEnd > length) {
    throw new IndexOutOfBoundsException();
  }
  flush();
  int index=length;
  for (Cell c=chain; c != null; c=c.next) {
    int start=index - c.value.length();
    int end=index;
    index=start;
    if (start < srcStart) {
      start=srcStart;
    }
    if (end > srcEnd) {
      end=srcEnd;
    }
    if (start < end) {
      c.value.getChars(start - index,end - index,dst,dstStart + (start - srcStart));
    }
  }
}
