{
  if (i < 0 || i >= length) {
    throw new IndexOutOfBoundsException();
  }
  flush();
  int index=length;
  for (Cell c=chain; c != null; c=c.next) {
    int start=index - c.value.length();
    index=start;
    if (i >= start) {
      return c.value.charAt(i - start);
    }
  }
  throw new RuntimeException();
}
