{
  int slength=s.length();
  if (slength == 0)   return lastIndex;
  for (int i=Math.min(length - slength,lastIndex); i >= 0; --i) {
    int j=0;
    for (; j < slength && i + j < length; ++j) {
      if (charAt(i + j) != s.charAt(j)) {
        break;
      }
    }
    if (j == slength) {
      return i;
    }
  }
  return -1;
}
