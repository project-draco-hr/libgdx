{
  int low=0;
  int high=values.length - 1;
  while (low <= high) {
    int midIndex=(low + high) >>> 1;
    int value=values[midIndex];
    int foundGlyphCode=value & 0xffff;
    if (foundGlyphCode < otherGlyphCode)     low=midIndex + 1;
 else     if (foundGlyphCode > otherGlyphCode)     high=midIndex - 1;
 else     return value >> 16;
  }
  return 0;
}
