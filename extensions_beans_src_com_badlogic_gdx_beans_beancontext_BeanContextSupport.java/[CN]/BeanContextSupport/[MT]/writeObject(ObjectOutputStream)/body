{
  boolean origSer=serializing;
  serializing=true;
  try {
    oos.defaultWriteObject();
    bcsPreSerializationHook(oos);
    if (this == getBeanContextPeer()) {
      writeChildren(oos);
    }
synchronized (bcmListeners) {
      serialize(oos,bcmListeners);
    }
  }
  finally {
    serializing=origSer;
  }
}
