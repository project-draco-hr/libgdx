{
  final float newX=w * this.x + x * this.w + y * this.z - z * y;
  final float newY=w * this.y + y * this.w + z * this.x - x * z;
  final float newZ=w * this.z + z * this.w + x * this.y - y * x;
  final float newW=w * this.w - x * this.x - y * this.y - z * z;
  this.x=newX;
  this.y=newY;
  this.z=newZ;
  this.w=newW;
  return this;
}
