{
  if (model == null)   throw new GdxRuntimeException("Call begin() first");
  final Model result=model;
  endnode();
  model=null;
  for (  final MeshBuilder mb : builders)   mb.end();
  builders.clear();
  for (  final MeshPart mp : result.meshParts) {
    if (!result.meshes.contains(mp.mesh,true))     result.meshes.add(mp.mesh);
  }
  return result;
}
