{
  cache.setColor(color.r,color.g,color.b,color.a * parentAlpha);
switch (valignment) {
case TOP:
    cache.setPosition(x,y + height - bounds.height);
  break;
case CENTER:
cache.setPosition(x,y + (height - bounds.height) / 2);
break;
case BOTTOM:
cache.setPosition(x,y);
break;
}
cache.draw(batch);
}
