{
  return this.set(min.set(min(min.x,sphere.center.x - sphere.radius),min(min.y,sphere.center.y - sphere.radius),min(min.z,sphere.center.z - sphere.radius)),max.set(max(max.x,sphere.center.x + sphere.radius),max(max.y,sphere.center.y + sphere.radius),max(max.z,sphere.center.z + sphere.radius)));
}
