{
  if (stashSize == stashCapacity) {
    resize(capacity << 1);
    put(key,value);
    return;
  }
  int[] keyTable=this.keyTable;
  for (int i=capacity, n=i + stashSize; i < n; i++) {
    if (keyTable[i] == key) {
      valueTable[i]=value;
      return;
    }
  }
  int index=capacity + stashSize;
  keyTable[index]=key;
  valueTable[index]=value;
  stashSize++;
}
