{
  int count=0;
  if (vmClass.methodTable != null) {
    for (int i=0; i < vmClass.methodTable.length; ++i) {
      if (((!publicOnly) || ((vmClass.methodTable[i].flags & Modifier.PUBLIC)) != 0) && Method.getName(vmClass.methodTable[i]).equals("<init>")) {
        ++count;
      }
    }
  }
  return count;
}
