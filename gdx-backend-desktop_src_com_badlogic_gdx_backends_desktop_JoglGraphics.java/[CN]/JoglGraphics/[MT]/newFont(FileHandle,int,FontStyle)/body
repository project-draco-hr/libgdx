{
  JoglFileHandle jHandle=(JoglFileHandle)file;
  InputStream in;
  try {
    in=new FileInputStream(jHandle.getFile());
    JoglFont font=new JoglFont(in,size,style);
    in.close();
    return font;
  }
 catch (  Exception e) {
    throw new GdxRuntimeException("Couldn't load font from file '" + file + "'",e);
  }
}
