{
  if (offset < 0 || offset + length > bytes.length) {
    throw new IndexOutOfBoundsException(offset + " < 0 or " + offset+ " + "+ length+ " > "+ bytes.length);
  }
  char[] c=new char[length];
  int mask=highByte << 8;
  for (int i=0; i < length; ++i) {
    c[i]=(char)((bytes[offset + i] & 0xFF) | mask);
  }
  this.data=c;
  this.offset=0;
  this.length=length;
}
