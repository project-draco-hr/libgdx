{
  int start=-1;
  for (int i=0; i < length; ++i) {
    char c=charAt(i);
    if (start == -1 && !Character.isWhitespace(c)) {
      start=i;
      break;
    }
  }
  int end=-1;
  for (int i=length - 1; i >= 0; --i) {
    char c=charAt(i);
    if (end == -1 && !Character.isWhitespace(c)) {
      end=i + 1;
      break;
    }
  }
  if (start >= end) {
    return "";
  }
 else {
    return substring(start,end);
  }
}
