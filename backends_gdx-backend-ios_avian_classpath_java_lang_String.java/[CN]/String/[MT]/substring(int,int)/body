{
  if (start >= 0 && end >= start && end <= length) {
    if (start == 0 && end == length) {
      return this;
    }
 else     if (end - start == 0) {
      return "";
    }
 else {
      return new String(data,offset + start,end - start,false);
    }
  }
 else {
    throw new IndexOutOfBoundsException(start + " not in [0, " + end+ ") or "+ end+ " > "+ length);
  }
}
