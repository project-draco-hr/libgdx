{
  for (int j=0; j < length; ++j) {
    char ch=charAt(j);
    if (Character.toUpperCase(ch) != ch) {
      char[] b=new char[length];
      for (int i=0; i < length; ++i) {
        b[i]=Character.toUpperCase(charAt(i));
      }
      return new String(b,0,length,false);
    }
  }
  return this;
}
