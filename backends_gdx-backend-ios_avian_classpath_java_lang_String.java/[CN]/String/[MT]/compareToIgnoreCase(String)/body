{
  if (this == s)   return 0;
  int idx=0;
  int result;
  int end=(length < s.length ? length : s.length);
  while (idx < end) {
    if ((result=Character.toLowerCase(charAt(idx)) - Character.toLowerCase(s.charAt(idx))) != 0) {
      return result;
    }
    idx++;
  }
  return length - s.length;
}
