{
  if (s.length == 0)   return lastIndex;
  for (int i=Math.min(length - s.length,lastIndex); i >= 0; --i) {
    int j=0;
    for (; j < s.length && i + j < length; ++j) {
      if (charAt(i + j) != s.charAt(j)) {
        break;
      }
    }
    if (j == s.length) {
      return i;
    }
  }
  return -1;
}
