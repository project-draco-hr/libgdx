{
  GLCommon gl=Gdx.gl;
  gl.glClear(GL10.GL_COLOR_BUFFER_BIT);
  gl.glEnable(GL10.GL_TEXTURE_2D);
  renderer.render();
  guiCam.update();
  batcher.setProjectionMatrix(guiCam.getCombinedMatrix());
  batcher.enableBlending();
  batcher.begin();
switch (state) {
case GAME_READY:
    presentReady();
  break;
case GAME_RUNNING:
presentRunning();
break;
case GAME_PAUSED:
presentPaused();
break;
case GAME_LEVEL_END:
presentLevelEnd();
break;
case GAME_OVER:
presentGameOver();
break;
}
batcher.end();
}
