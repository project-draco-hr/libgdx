{
  super(text,style);
  clearChildren();
  Label label=getLabel();
  if (checkBoxLeft) {
    imageCell=add(image=new Image(style.checkboxOff));
    add(label);
  }
 else {
    add(label);
    imageCell=add(image=new Image(style.checkboxOff));
  }
  label.setAlignment(Align.left);
  setSize(getPrefWidth(),getPrefHeight());
}
