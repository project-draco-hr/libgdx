{
  if (addendum != null && addendum.annotationTable instanceof byte[]) {
    try {
      addendum.annotationTable=parseAnnotationTable(loader,addendum.pool,new ByteArrayInputStream((byte[])addendum.annotationTable));
    }
 catch (    IOException e) {
      AssertionError error=new AssertionError();
      error.initCause(e);
      throw error;
    }
  }
}
