{
  int result;
  int end;
switch (spec[start]) {
case 'L':
    ++start;
  end=start;
while (spec[end] != ';') ++end;
result=end + 1;
break;
case '[':
end=start + 1;
while (spec[end] == '[') ++end;
switch (spec[end]) {
case 'L':
++end;
while (spec[end] != ';') ++end;
++end;
break;
default :
++end;
}
result=end;
break;
default :
return start + 1;
}
loadVMClass(loader,spec,start,end - start);
return result;
}
