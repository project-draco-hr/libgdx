{
  long[] bits=this.bits;
  for (int word=bits.length - 1; word >= 0; --word) {
    long bitsAtWord=bits[word];
    if (bitsAtWord != 0) {
      for (int bit=63; bit >= 0; --bit) {
        if ((bitsAtWord & (1L << (bit & 0x3F))) != 0L) {
          return (word << 6) + bit;
        }
      }
    }
  }
  return 0;
}
