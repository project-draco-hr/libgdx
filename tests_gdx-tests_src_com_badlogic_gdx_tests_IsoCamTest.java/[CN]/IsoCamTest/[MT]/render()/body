{
  Gdx.gl.glClear(GL10.GL_COLOR_BUFFER_BIT);
  cam.update();
  batch.setProjectionMatrix(cam.combined);
  batch.setTransformMatrix(matrix);
  batch.begin();
  for (int z=0; z < 10; z++) {
    for (int x=0; x < 10; x++) {
      sprites[x][z].draw(batch);
    }
  }
  batch.end();
  checkTileTouched();
}
