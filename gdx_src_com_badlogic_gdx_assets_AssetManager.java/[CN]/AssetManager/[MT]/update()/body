{
  try {
    if (tasks.size() == 0) {
      if (preloadQueue.size == 0)       return true;
      nextTask();
      if (preloadQueue.size == 0 || tasks.size() == 0)       return true;
    }
    return updateTask() && preloadQueue.size == 0;
  }
 catch (  Throwable t) {
    handleTaskError(t);
    return preloadQueue.size == 0;
  }
}
