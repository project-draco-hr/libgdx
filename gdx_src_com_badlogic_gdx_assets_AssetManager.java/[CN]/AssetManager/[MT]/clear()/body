{
  if (preloadQueue.size > 0 || tasks.size() != 0) {
    try {
      while (!updateTask())       ;
    }
 catch (    Throwable t) {
      handleTaskError(t);
    }
  }
  Array<String> assets=assetTypes.keys().toArray();
  for (  String asset : assets) {
    remove(asset);
  }
  this.assets.clear();
  this.assetTypes.clear();
  this.assetDependencies.clear();
  this.loaded=0;
  this.preloadQueue.clear();
  this.tasks.clear();
}
