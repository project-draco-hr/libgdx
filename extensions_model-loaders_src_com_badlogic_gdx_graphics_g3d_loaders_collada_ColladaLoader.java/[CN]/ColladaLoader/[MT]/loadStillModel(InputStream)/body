{
  Xml xml=new Xml();
  Element root=null;
  try {
    root=xml.parse(in);
  }
 catch (  Exception e) {
    throw new GdxRuntimeException("Couldn't load Collada model",e);
  }
  Array<Geometry> geos=readGeometries(root);
  StillSubMesh[] meshes=createMeshes(geos);
  for (  StillSubMesh mesh : meshes) {
    System.out.println(mesh.mesh.getNumVertices());
  }
  StillModel model=new StillModel();
  model.subMeshes=meshes;
  return model;
}
