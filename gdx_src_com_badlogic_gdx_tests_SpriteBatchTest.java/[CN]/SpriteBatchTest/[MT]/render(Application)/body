{
  GL10 gl=app.getGraphics().getGL10();
  gl.glClear(GL.GL_COLOR_BUFFER_BIT);
  spriteBatch.begin();
  renderSpriteBatch();
  spriteBatch.end();
  if (System.nanoTime() - startTime > 1000000000) {
    app.log("SpriteBatch","fps: " + frames);
    frames=0;
    startTime=System.nanoTime();
  }
  frames++;
}
