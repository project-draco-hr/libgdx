{
  if (suggestedShader != null && suggestedShader.canRender(instance.material))   return suggestedShader;
  for (int i=0; i < shaders.size; i++) {
    final Shader shader=shaders.get(i);
    if (shader.canRender(instance.material))     return shader;
  }
  final Shader result=createShader(instance.material);
  shaders.add(result);
  return result;
}
