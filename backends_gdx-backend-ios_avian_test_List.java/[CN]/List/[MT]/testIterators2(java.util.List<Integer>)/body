{
  l.add(1);
  l.add(2);
  l.add(3);
  ListIterator<Integer> it=l.listIterator();
  expect(it.next().equals(Integer.valueOf(1)));
  it.remove();
  expect(it.next().equals(Integer.valueOf(2)));
  it.remove();
  expect(it.next().equals(Integer.valueOf(3)));
  it.remove();
  expect(!it.hasNext());
  expect(l.isEmpty());
  l.add(1);
  l.add(2);
  l.add(3);
  it=l.listIterator(1);
  expect(it.next().equals(Integer.valueOf(2)));
  it.remove();
  expect(it.next().equals(Integer.valueOf(3)));
  it.remove();
  expect(!it.hasNext());
  expect(l.size() == 1);
  l.add(2);
  l.add(3);
  it=l.listIterator(2);
  expect(it.next().equals(Integer.valueOf(3)));
  it.remove();
  expect(!it.hasNext());
  expect(l.size() == 2);
  l.add(3);
  it=l.listIterator(3);
  expect(it.previous().equals(Integer.valueOf(3)));
  it.remove();
  expect(it.previous().equals(Integer.valueOf(2)));
  it.remove();
  expect(it.previous().equals(Integer.valueOf(1)));
  it.remove();
  expect(!it.hasPrevious());
  expect(l.isEmpty());
  l.add(1);
  l.add(2);
  l.add(3);
  it=l.listIterator(2);
  expect(it.previous().equals(Integer.valueOf(2)));
  it.remove();
  expect(it.previous().equals(Integer.valueOf(1)));
  it.remove();
  expect(!it.hasPrevious());
  expect(l.size() == 1);
  l.clear();
  l.add(1);
  l.add(2);
  l.add(3);
  it=l.listIterator(1);
  expect(it.previous().equals(Integer.valueOf(1)));
  it.remove();
  expect(!it.hasPrevious());
  expect(l.size() == 2);
}
