{
  updateCorners();
  this.inf();
  for (  Vector l_pnt : crn) {
    l_pnt.mul(a_matrix);
    min.set(Math.min(min.getX(),l_pnt.getX()),Math.min(min.getY(),l_pnt.getY()),Math.min(min.getZ(),l_pnt.getZ()));
    max.set(Math.max(max.getX(),l_pnt.getX()),Math.max(max.getY(),l_pnt.getY()),Math.max(max.getZ(),l_pnt.getZ()));
  }
  crn_dirty=true;
  return this.set(min,max);
}
