{
  crn_dirty=true;
  return this.set(min.set(Math.min(min.getX(),a_bounds.min.getX()),Math.min(min.getY(),a_bounds.min.getY()),Math.min(min.getZ(),a_bounds.min.getZ())),max.set(Math.max(max.getX(),a_bounds.max.getX()),Math.max(max.getY(),a_bounds.max.getY()),Math.max(max.getZ(),a_bounds.max.getZ())));
}
