{
  float l_mat[]=matrix.val;
  float l_w=x * l_mat[Matrix.M30] + y * l_mat[Matrix.M31] + z * l_mat[Matrix.M32] + l_mat[Matrix.M33];
  return this.set((x * l_mat[Matrix.M00] + y * l_mat[Matrix.M01] + z * l_mat[Matrix.M02] + l_mat[Matrix.M03]) / l_w,(x * l_mat[Matrix.M10] + y * l_mat[Matrix.M11] + z * l_mat[Matrix.M12] + l_mat[Matrix.M13]) / l_w,(x * l_mat[Matrix.M20] + y * l_mat[Matrix.M21] + z * l_mat[Matrix.M22] + l_mat[Matrix.M23]) / l_w);
}
