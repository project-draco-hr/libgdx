{
  dynamicsWorld.stepSimulation(Gdx.graphics.getDeltaTime(),5);
  GL10 gl=Gdx.gl10;
  for (int i=0; i < entities.size; i++) {
    final Entity body=entities.get(i);
    gl.glPushMatrix();
    gl.glMultMatrixf(body.transform.val,0);
    body.mesh.render(GL10.GL_TRIANGLE_STRIP);
    gl.glPopMatrix();
  }
}
