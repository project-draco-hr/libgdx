{
  dynamicsWorld.stepSimulation(Gdx.graphics.getDeltaTime(),5);
  GL10 gl=Gdx.gl10;
  for (int i=0; i < entities.size; i++) {
    final Entity entity=entities.get(i);
    gl.glPushMatrix();
    gl.glMultMatrixf(entity.transform.val,0);
    entity.mesh.render(GL10.GL_TRIANGLES);
    gl.glPopMatrix();
  }
}
