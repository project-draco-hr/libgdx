{
  StringBuilder sb=new StringBuilder();
  for (Iterator<Map.Entry> it=map.entrySet().iterator(); it.hasNext(); ) {
    Map.Entry e=it.next();
    sb.append(e.getKey());
    sb.append("=");
    sb.append(e.getValue());
    if (it.hasNext()) {
      sb.append(", ");
    }
  }
  return sb.toString();
}
