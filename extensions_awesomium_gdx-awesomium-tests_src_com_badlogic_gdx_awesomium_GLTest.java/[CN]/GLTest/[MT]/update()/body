{
  webCore.update();
  if (webView.isDirty()) {
    RenderBuffer renderBuffer=webView.render();
    if (renderBuffer != null) {
      texture.bind();
      Gdx.gl.glTexImage2D(GL10.GL_TEXTURE_2D,0,GL10.GL_RGBA,512,512,0,0x80E1,GL10.GL_UNSIGNED_BYTE,renderBuffer.getBuffer());
    }
  }
  angle+=Gdx.graphics.getDeltaTime() * 20 * angleInc;
  if (angle > 20) {
    angle=20;
    angleInc=-1;
  }
  if (angle < -20) {
    angle=-20;
    angleInc=1;
  }
  matrix.setToRotation(yAxis,angle);
  inv.set(matrix).inv();
}
