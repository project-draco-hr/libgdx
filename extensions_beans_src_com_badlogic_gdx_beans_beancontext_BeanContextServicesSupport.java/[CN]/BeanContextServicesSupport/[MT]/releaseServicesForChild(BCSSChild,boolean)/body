{
  if (bcssChild.serviceRecords == null || bcssChild.serviceRecords.isEmpty()) {
    return;
  }
synchronized (bcssChild.child) {
    Object records[]=bcssChild.serviceRecords.toArray();
    for (int i=0; i < records.length; i++) {
      ServiceRecord rec=(ServiceRecord)records[i];
      if (delegatedServices) {
        if (rec.isDelegate) {
          releaseServiceWithoutCheck(rec.child,bcssChild,rec.requestor,rec.service,true);
        }
      }
 else {
        releaseServiceWithoutCheck(rec.child,bcssChild,rec.requestor,rec.service,false);
      }
    }
  }
}
