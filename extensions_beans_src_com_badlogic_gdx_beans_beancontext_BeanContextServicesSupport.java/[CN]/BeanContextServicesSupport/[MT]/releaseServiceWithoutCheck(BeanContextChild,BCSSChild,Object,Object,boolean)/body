{
  if (bcssChild.serviceRecords == null || bcssChild.serviceRecords.isEmpty()) {
    return;
  }
synchronized (child) {
    for (Iterator iter=bcssChild.serviceRecords.iterator(); iter.hasNext(); ) {
      ServiceRecord rec=(ServiceRecord)iter.next();
      if (rec.requestor == requestor && rec.service == service) {
        rec.provider.releaseService(getBeanContextServicesPeer(),requestor,service);
        if (callRevokedListener && rec.revokedListener != null) {
          rec.revokedListener.serviceRevoked(new BeanContextServiceRevokedEvent(getBeanContextServicesPeer(),rec.serviceClass,true));
        }
        iter.remove();
        break;
      }
    }
  }
}
