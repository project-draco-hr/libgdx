{
  OutputStream output=null;
  try {
    output=write(append);
    StreamUtils.copyStream(input,output,4096);
  }
 catch (  Exception ex) {
    throw new GdxRuntimeException("Error stream writing to file: " + file + " ("+ type+ ")",ex);
  }
 finally {
    StreamUtils.closeQuietly(input);
    StreamUtils.closeQuietly(output);
  }
}
