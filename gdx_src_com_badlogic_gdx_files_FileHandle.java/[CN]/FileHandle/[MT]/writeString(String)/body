{
  OutputStream writeStream=write(false);
  try {
    writeStream.write(string.getBytes("UTF8"));
  }
 catch (  Exception e) {
    throw new GdxRuntimeException("Couldn't write string to file '" + path() + "'",e);
  }
 finally {
    if (writeStream != null)     try {
      writeStream.close();
    }
 catch (    Exception e) {
    }
  }
}
