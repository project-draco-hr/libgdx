{
  clear();
  padTop=Value.zero;
  padLeft=Value.zero;
  padBottom=Value.zero;
  padRight=Value.zero;
  align=Align.center;
  debug(Debug.none);
  cellDefaults.defaults();
  for (int i=0, n=columnDefaults.size; i < n; i++) {
    Cell columnCell=columnDefaults.get(i);
    if (columnCell != null)     cellPool.free(columnCell);
  }
  columnDefaults.clear();
}
