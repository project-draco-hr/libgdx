{
  super(url);
  String s=url.getFile();
  int index=s.indexOf("!/");
  try {
    this.file=new JarFile(new URL(s.substring(0,index)).getFile());
    this.entry=this.file.getJarEntry(s.substring(index + 2));
  }
 catch (  MalformedURLException e) {
    throw new RuntimeException(e);
  }
catch (  IOException e) {
    throw new RuntimeException(e);
  }
}
