{
  float l_mat[]=matrix.val;
  return this.set(x * l_mat[Matrix.M00] + y * l_mat[Matrix.M01] + z * l_mat[Matrix.M02] + l_mat[Matrix.M03],x * l_mat[Matrix.M10] + y * l_mat[Matrix.M11] + z * l_mat[Matrix.M12] + l_mat[Matrix.M13],x * l_mat[Matrix.M20] + y * l_mat[Matrix.M21] + z * l_mat[Matrix.M22] + l_mat[Matrix.M23]);
}
