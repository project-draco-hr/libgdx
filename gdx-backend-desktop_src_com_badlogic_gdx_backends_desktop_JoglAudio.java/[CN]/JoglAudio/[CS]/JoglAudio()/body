{
  try {
    AudioFormat format=new AudioFormat(44100.0f,16,2,true,false);
    line=AudioSystem.getSourceDataLine(format);
    line.open(format,4410);
    line.start();
    thread=new Thread(this);
    thread.setDaemon(true);
    thread.start();
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
}
