{
  if (world.renderMeshes) {
    MeshPart meshPart=model.nodes.get(0).parts.get(0).meshPart;
    softBody.getVertices(meshPart.mesh.getVerticesBuffer(),meshPart.mesh.getVertexSize(),meshPart.mesh.getVertexAttribute(Usage.Position).offset,meshPart.mesh.getIndicesBuffer(),meshPart.indexOffset,meshPart.numVertices,indexMap,0);
    softBody.getWorldTransform(entity.transform);
  }
  super.render();
}
