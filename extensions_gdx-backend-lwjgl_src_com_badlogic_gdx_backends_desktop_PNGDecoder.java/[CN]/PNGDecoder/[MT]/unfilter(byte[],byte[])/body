{
switch (curLine[0]) {
case 0:
    break;
case 1:
  unfilterSub(curLine);
break;
case 2:
unfilterUp(curLine,prevLine);
break;
case 3:
unfilterAverage(curLine,prevLine);
break;
case 4:
unfilterPaeth(curLine,prevLine);
break;
default :
throw new IOException("invalide filter type in scanline: " + curLine[0]);
}
}
