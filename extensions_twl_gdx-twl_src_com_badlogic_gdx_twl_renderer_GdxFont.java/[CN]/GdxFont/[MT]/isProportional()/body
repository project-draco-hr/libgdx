{
  if (proportional == null) {
    try {
      int iBound=(int)bitmapFont.getBounds("i").width;
      int mBound=(int)bitmapFont.getBounds("m").width;
      proportional=iBound != 0 && mBound != 0 && iBound != mBound;
    }
 catch (    Exception e) {
      proportional=false;
    }
  }
  return proportional;
}
