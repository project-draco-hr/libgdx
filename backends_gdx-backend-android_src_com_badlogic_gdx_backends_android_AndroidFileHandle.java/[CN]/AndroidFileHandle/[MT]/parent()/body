{
  File parent=file.getParentFile();
  if (parent == null) {
switch (type) {
case Classpath:
case Absolute:
      parent=new File("/");
    break;
case Internal:
  parent=new File("");
break;
case External:
parent=new File(Gdx.files.getExternalStoragePath());
break;
}
}
return new AndroidFileHandle(assets,parent,type);
}
