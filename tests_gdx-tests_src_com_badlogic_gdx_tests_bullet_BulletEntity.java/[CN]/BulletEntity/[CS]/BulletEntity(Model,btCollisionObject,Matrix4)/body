{
  this.model=model;
  this.transform.set(transform);
  this.body=body;
  if (body != null) {
    body.userData=this;
    if (body instanceof btRigidBody) {
      this.motionState=new MotionState(this.transform);
      ((btRigidBody)this.body).setMotionState(motionState);
    }
 else     body.setWorldTransform(transform);
  }
}
