{
  if (peer != 0) {
    if (b == null) {
      throw new NullPointerException();
    }
    if (offset < 0 || offset + length > b.length) {
      throw new ArrayIndexOutOfBoundsException();
    }
    int c=read(peer,position,b,offset,length);
    if (c >= 0) {
      position+=c;
    }
    return c;
  }
 else {
    throw new IOException();
  }
}
