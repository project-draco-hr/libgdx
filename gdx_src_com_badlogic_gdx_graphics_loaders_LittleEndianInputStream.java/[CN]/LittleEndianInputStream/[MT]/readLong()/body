    throw new EOFException();
  }
  return (byte8 << 56) + ((byte7 << 56) >>> 8) + ((byte6 << 56) >>> 16)+ ((byte5 << 56) >>> 24)+ ((byte4 << 56) >>> 32)+ ((byte3 << 56) >>> 40)+ ((byte2 << 56) >>> 48)+ ((byte1 << 56) >>> 56);
}
