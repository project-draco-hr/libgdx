{
  this.voxels=new byte[width * height * depth];
  this.width=width;
  this.height=height;
  this.depth=depth;
  this.topOffset=width * depth;
  this.bottomOffset=-width * depth;
  this.leftOffset=-1;
  this.rightOffset=1;
  this.frontOffset=-width;
  this.backOffset=width;
  this.widthTimesHeight=width * height;
}
