{
  try {
    BufferedReader br=new BufferedReader(new InputStreamReader(ExpositionScreen.class.getResourceAsStream("exposition.txt")));
    String line="";
    while ((line=br.readLine()) != null) {
      lines.add(line);
    }
    br.close();
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
}
