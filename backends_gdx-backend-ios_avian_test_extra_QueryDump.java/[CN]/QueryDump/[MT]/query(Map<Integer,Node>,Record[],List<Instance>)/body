{
  if (stack.size() > 1) {
    Instance instance=peek(stack,1);
    if (instance != null && instance.record == query[0]) {
      Node node=nodes.get(instance.key);
      if (node == null) {
        nodes.put(instance.key,new LeafNode(instance));
        query(nodes,query,stack,1);
      }
      return;
    }
  }
  query(nodes,query,stack,0);
}
